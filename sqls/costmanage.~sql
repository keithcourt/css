SELECT t.aaz434,
       t.aie130,
       t.aae100,
       a.aaz430,
       t.aaz477,
       a.aie138,
       a.aie137,
       a.aie134,
       a.aie136,
       a.aie133,
       a.aie076,
       b.aaz402,
       b.aie123*100 as aie123
  FROM id33 t,id24 a,id34 b
where t.aaz434=b.aaz434 
  and b.aaz430=a.aaz430 
  and t.aaz434=#aaz434#
                  
                  
