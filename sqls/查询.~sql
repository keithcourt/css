<!--实账账户-->
SELECT c.aac002,
       c.aac003,
       a.aaa119,
       a.aac001,
       a.aab001,
       a.aae450,
       a.aaz415,
       a.aaz430,
       a.aaz434,
       decode(a.aie171,'2','0.0000','3','0.0000',a.aie170) aie170,
       a.aie171,
       a.aae460,
       TRUNC(a.aie170 * b.aie134,2) aae240,
       b.aie134,
       decode(b.aie136,'20141001','',b.aie136) aie136,
       d.aie080,
       d.aie077,
       b.aie133,
       b.aie138
  FROM id40 a,id24 b,ac01 c,id33 d
 WHERE a.aaz430=b.aaz430 
   AND a.aaz434=d.aaz434 
   AND a.aae100='1' 
   AND a.aac001=c.aac001


    builder.appendNotBlank(" and d.aaa027 = :aaa027", "aaa027", QueryFactory.NONE_LIKE);// 行政区划
    builder.appendNotBlank(" and c.aac002 = :aac002", "aac002", QueryFactory.NONE_LIKE);// 社会保障号码
    builder.appendNotBlank(" and c.aac003 = :aac003", "aac003", QueryFactory.NONE_LIKE);// 姓名
    builder.appendNotBlank(" and a.aac001 = :aac001", "aac001", QueryFactory.NONE_LIKE);// 个人ID
    builder.appendNotBlank(" and a.aab001 = :aab001", "aab001", QueryFactory.NONE_LIKE);// 单位
    builder.appendNotBlank(" and a.aaa119 = :aaa119", "aaa119", QueryFactory.NONE_LIKE);// 账户类型

    builder.append(" order by c.aac003,c.aac002,b.aaz430 ");
<!--个人实账变动明细-->
